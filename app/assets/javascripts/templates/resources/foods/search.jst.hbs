<div class="live-data-form">
  <div class="search-form-and-results">
    <form class="form-search">
      <img id="food-search-spinner" class="spinner" src="/assets/spinner.gif" style="visibility:hidden;" />
      <input type="text" id="food-search-term" class="input-medium search-query" placeholder="Search for Food">
      <button type="submit" id="food-search-btn" class="btn"><i class="icon-search"></i></button>
    </form>
    <ul id="food-search-results" class="search-results">
      <li style="text-align:center;"><div class="muted">Food Search Results</div></li>
    </ul>
  </div>
  <div id="selected-food-details" class="selected-item-view">
    {{> resources/foods/_search_instructions }}
  </div>
</div>
{{> common/_live_data_footer }}
